class Solution {
    public int diagonalSum(int[][] mat) {
int n = mat.length;
      int lsum =0;
      for(int i=0;i<mat.length;i++){
          for(int j=0;j<mat[0].length;j++){
              if(i==j||i+j==mat.length-1)
              lsum+=mat[i][j];
          }
      }  lsum = lsum;
      return lsum;
    }
}
