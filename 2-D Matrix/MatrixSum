class Solution
{
    public void  Addition(int[][] matrixA, int[][] matrixB)
    {
        // code here
        int n = matrixA.length;
        int m = matrixA[0].length;
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++)
            {
                matrixA[i][j]+=matrixB[i][j];
            }
        }
    }
}
